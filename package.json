{
  "name": "javan-rhino",
  "private": true,
  "version": "1.0.0",
  "description": "Front-end to Ubuntu Store Payments",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently --kill-others \"npm run watch-client\" \"node server/\"",
    "start-prod": "NODE_ENV=production node dist/server/",
    "build": "./scripts/build.sh",
    "watch-client": "node webpack/webpack-dev-server.js",
    "lint": "eslint -c .eslintrc.js '**/*.js'; exit 0",
    "test": "nyc npm run test:run && npm run lint",
    "test:run": "npm run test:unit && npm run test:routes",
    "test:unit": "BABEL_DISABLE_CACHE=1 NODE_ENV=TEST mocha --require babel-core/register --compilers js:babel-register ./test/unit/setup.js ./test/unit/**/*",
    "test:routes": "BABEL_DISABLE_CACHE=1 NODE_ENV=TEST mocha --require babel-core/register --compilers js:babel-register ./test/routes/setup.js ./test/routes/**/*",
    "test:watch": "npm run test:unit -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canonical-ols/javan-rhino.git"
  },
  "contributors": [
    "Bartek Szopka <bartek.szopka@canonical.com>",
    "Katie Fenn <katie.fenn@canonical.com>",
    "Stephen Stewart <stephen.stewart@canonical.com>"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/canonical-ols/javan-rhino/issues"
  },
  "homepage": "https://github.com/canonical-ols/javan-rhino#readme",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "concurrently": "^2.2.0",
    "connect-mongo": "^1.3.2",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.0",
    "eslint-plugin-react": "^6.1.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "macaroons.js": "^0.3.6",
    "mocha": "^3.0.2",
    "nconf": "^0.8.4",
    "nconf-yaml": "^1.0.2",
    "nock": "^8.0.0",
    "normalize.css": "^4.2.0",
    "nyc": "^8.1.0",
    "openid": "^2.0.1",
    "postcss-loader": "^0.10.0",
    "postcss-simple-vars": "^3.0.0",
    "qs": "^6.2.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-mock-store": "^1.1.4",
    "request": "^2.74.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-isomorphic-tools": "^2.5.7"
  }
}
